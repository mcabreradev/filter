{
  "entryPoints": ["src/index.ts"],
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "hideGenerator": true,
  "disableSources": false,
  "includeVersion": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Function",
    "Interface",
    "TypeAlias",
    "Variable",
    "Class"
  ],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Operators",
    "Types",
    "Config",
    "Utils",
    "*"
  ]
}

